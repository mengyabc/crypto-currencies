{
  "name": "crypto-currencies",
  "version": "v0.1.0",
  "description": "Crypto Currencies Analysis",
  "main": "main.js",
  "author": "Mengya",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@github.com:mengyabc/crypto-currencies.git"
  },
  "scripts": {
    "start": "NODE_ENV=development node main.js",
    "start:prod": "npm run build && NODE_ENV=production node main.js",
    "build": "webpack --config webpack.prod.config.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint --ext .jsx,.js ./",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:clear": "NODE_ENV=test jest --clearCache",
    "test:updateSnapshot": "NODE_ENV=test jest --updateSnapshot"
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupTestFrameworkScriptFile": "<rootDir>/app/enzyme-setup.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^assets(.*)$": "<rootDir>/builds/assets$1",
      "^layouts(.*)$": "<rootDir>/app/layouts$1",
      "^components(.*)$": "<rootDir>/app/components$1",
      "^scss(.*)$": "<rootDir>/app/scss$1",
      "^redux_and_actions(.*)$": "<rootDir>/app/redux_and_actions$1",
      "^redux-mock-store(.*)$": "<rootDir>/app/__mocks__/redux-mock-store.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/__mocks__/styleMock.js"
    }
  },
